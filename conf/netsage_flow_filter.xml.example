<config>

  <!-- rabbitmq connection info -->
  <rabbit_input>
    <host>127.0.0.1</host>
    <port>5672</port>
    <username>guest</username>
    <password>guest</password>
    <ssl>0</ssl>
    <batch_size>100</batch_size>
    <vhost>/</vhost>
    <cacert>/path/to/certificate.crt</cacert> <!-- required if ssl is 1 -->
    <queue>netsage_deidentifier_raw_prefilter</queue>
    <channel>2</channel>
    <durable>1</durable> <!-- Whether the rabbit queue is 'durable' (don't change this unless you have a reason) -->
  </rabbit_input>
  <rabbit_output>
    <host>127.0.0.1</host>
    <port>5672</port>
    <username>guest</username>
    <password>guest</password>
    <ssl>0</ssl>
    <batch_size>100</batch_size>
    <vhost>/</vhost>
    <cacert>/path/to/certificate.crt</cacert> <!-- required if ssl is 1 -->
    <channel>3</channel>
    <queue>netsage_deidentifier_raw</queue>
    <durable>1</durable> <!-- Whether the rabbit queue is 'durable' (don't change this unless you have a reason) -->
  </rabbit_output>
  <simp>
      <host>127.0.0.1</host>
      <port>5672</port>
      <username>guest</username>
      <password>guest</password>
      <exchange>Simp</exchange>
      <timeout>60</timeout>
      <topic>Simp.Data</topic>
  </simp>
  <worker>
    <!-- How many flows to process at once -->
    <flow-batch-size>100</flow-batch-size>
    <!-- How many concurrent workers should perform the necessary operations -->
    <num-processes>1</num-processes>

    <!-- router address -->
    <router-address>ROUTER_IP_ADDRESS</router-address>

    <!-- how long to cache router SNMP data, in seconds (defaults to 3600) -->
    <!-- <snmp-cache-time>7200</snmp-cache-time> -->

  </worker>
  <master>
    <!-- where should we write the daemon pid file to -->
    <pid-file>/var/run/netsage-flow-filter-daemon.pid</pid-file>
  </master>
</config>
