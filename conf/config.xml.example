<config>
  <!-- rabbitmq connection info -->
  <rabbit>
    <host>127.0.0.1</host>
    <port>5671</port>
    <ssl>1</ssl>
    <cacert>/path/to/ca_certificate.crt</cacert> <!-- required if ssl is enabled -->
    <batch_size>100</batch_size>
    <vhost>netsage</vhost>
    <queue name="tstat" rabbit_name="netsage_tstat" channel="1" />
    <queue name="raw" rabbit_name="netsage_anonymizer_raw_dummy" channel="2" />
    <queue name="tagged" rabbit_name="netsage_anonymizer_tagged" channel="3" />
    <queue name="anonymized" rabbit_name="netsage_anonymizer_anonymized" channel="4" />
  </rabbit>
</config>
